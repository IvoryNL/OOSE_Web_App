@using Logic.Models
@{
    ViewData["Title"] = "Onderwijs";
}
@model Leeruitkomst
<div class="onderwijs-page">
    <h3>Leeruitkomst</h3>
    <div class="content">
        <table>
            <tr>
                <td>@Html.LabelFor(m => m.Naam)</td>
                <td>@Html.DisplayFor(m => m.Naam)</td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.Beschrijving)</td>
                <td>@Html.DisplayTextFor(m => m.Beschrijving)</td>
            </tr>
        </table>
    </div>
</div>